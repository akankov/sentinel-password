{
  "name": "@sentinel-password/react",
  "version": "0.2.1",
  "type": "module",
  "description": "React hooks and components for @sentinel-password/core - password validation made simple",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "password",
    "validation",
    "react",
    "hooks",
    "components",
    "typescript",
    "password-validator",
    "password-strength",
    "react-hooks"
  ],
  "author": "Aleksei Kankov <akankov@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/akankov/sentinel-password",
    "directory": "packages/react"
  },
  "homepage": "https://github.com/akankov/sentinel-password#readme",
  "bugs": {
    "url": "https://github.com/akankov/sentinel-password/issues"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@sentinel-password/core": "workspace:*"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.6.15",
    "@storybook/addon-interactions": "^8.6.15",
    "@storybook/blocks": "^8.6.15",
    "@storybook/react": "^10.1.10",
    "@storybook/react-vite": "^10.1.10",
    "@storybook/test": "^8.6.15",
    "@testing-library/react": "^16.3.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitest/coverage-v8": "^4.0.16",
    "jsdom": "^27.4.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "storybook": "^10.1.10",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  }
}
